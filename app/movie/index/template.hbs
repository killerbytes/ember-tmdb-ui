<h1>Movie</h1>
{{link-to "Now Playing" "movie" (query-params list="now_playing" page=1)}}
{{link-to "Popular" "movie" (query-params list="popular" page=1)}}
{{link-to "Top Rated" "movie" (query-params list="top_rated" page=1)}}
{{link-to "Upcoming" "movie" (query-params list="upcoming" page=1)}}

<ul>
  {{#each movies.results   as |item|}}
  <li>
    {{link-to item.title "movie.detail" item.id}}
  </li>
  {{/each}}
</ul>

{{tmdb-pager model=movies params=params }}
